   (@PnParametro        Integer,
    @PsColumna          Sysname)
Returns Varchar(500)

Begin
   Declare
      @o_Salida  Varchar ( 500)

   Begin
      If @PsColumna Not In ('valor', 'cadena')
         Begin
            Set @o_Salida = 'Error en Selección'
            Return (@o_Salida)
         End

      If @PsColumna = 'cadena'
         Begin
            Select @o_Salida = cadena
            From   dbo.parametros With (Nolock)
            Where  id = @PnParametro
         End
      Else
         Begin
            Select @o_Salida = Cast(Valor As Varchar)
            From   dbo.parametros With (Nolock)
            Where  id = @PnParametro
         End

      Return (@o_Salida)

   End

End
