Begin
   Set @ReturnCode = 0

/****** Object:  JobCategory [[Uncategorized (Local)]]    Script Date: 18/09/2024 01:47:21 p. m. ******/

   Begin Transaction
      If Not Exists (Select name
                     From   msdb.dbo.syscategories
                     Where  name          = N'[Uncategorized (Local)]'
